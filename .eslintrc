{
  "extends": "eslint-config-airbnb-es5",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "rules": {
    "func-names": [
      2,
      "never"
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "globals": {
    "require": true
  }
}
